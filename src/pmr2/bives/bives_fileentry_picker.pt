<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      metal:use-macro="context/@@pmr2-z3cform-macros/main">

<metal:slot metal:fill-slot="style_slot"
    i18n:domain="pmr2.bives">

  <style type="text/css"
      tal:content="string:@import url($portal_url/++resource++pmr2.cytoscapejs.css/main.css);">
  </style>

</metal:slot>

<!--
Remove these once these are properly defined to exist along with the
config.js script.
-->
<metal:slot metal:fill-slot="javascript_head_slot" 
    i18n:domain="pmr2.bives">

  <script type="text/javascript"
      tal:attributes="src string:$portal_url/++resource++pmr2.cytoscapejs.js/lib/arbor.js">
  </script>

  <script type="text/javascript"
      tal:attributes="src string:$portal_url/++resource++pmr2.cytoscapejs.js/build/cytoscape.min.js">
  </script>

</metal:slot>

<body>

<metal:slot metal:fill-slot="main" i18n:domain="cmf_default">
<div id="layout-contents">
<div id="bives_fileentry">
<table>
<thead>
<tr>
<th>Workspace</th>
<th>Path</th>
<th>Pick As Source</th>
<th>Pick As Target</th>
</tr>
<thead>
<tbody>
</tbody>
</table>
</div>
</div>
<button class="btn" id="btn_bives_fileentry_pick">Compare</button>
<form id="bives_call" method="post">
<input id="form-widgets-file1" name="form.widgets.file1" type="hidden" />
<input id="form-widgets-file2" name="form.widgets.file2" type="hidden" />
<input id="form-buttons-compare" name="form.buttons.compare" type="hidden" value="1"/>
<span tal:replace="structure context/@@authenticator/authenticator" />
</form>
</metal:slot>

</body>
</html>
